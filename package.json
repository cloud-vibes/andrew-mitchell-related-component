    "apollo-server-express": "^2.4.8",
    "aws-sdk": "^2.401.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-airbnb": "^3.2.0",
    "cors": "^2.8.5",
